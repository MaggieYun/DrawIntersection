/**
 * 区域事件
 * @class
 * @constructor
 * @param {String} type 类型
 * @param {Array} extent    当前区域
 * @param {Boolean} levelChange 等级（或比例尺）是否发生变化
 */
var ExtentEvent = function(type,extent,levelChange){
    /**
     * 事件类型
     * @type {String}
     */
    this.type = type;
    /**
     * 当前区域
     * @type {Array}
     */
    this.extent = extent;
    /**
     * 等级（或比例尺）是否发生变化
     * @type {Boolean}
     */
    this.levelChange = levelChange;
};
/**
 * 区域变化事件
 * @constant
 * @type String
 */
ExtentEvent.EXTENT_CHANGE = "extentChange";