
	
def find_best_path(start,end):
	openlist=[]
	closelist=[]
	nowpoint=none
	openlist.push(start)
	flag=true
	while flag:
		nowpoint=get_nowpoint(end)
		closelist=add_closelist(nowpoint)
		nowpoint.get_neighborpoint

		for x in range(nowpoint.neighborpoint):
			if (x in closelist):
				continue
			if(x in openlist):
				for i in range(lines):
					if((nowpoint.id==i[0] and x.id==i[1]) or (nowpoint.id==i[1] and x.id==i[0])):
						temp_g=i[3]
				if((nowpoint.g+temp_g)<x.g):
					x.set_parent(nowpoint)
					x.get_g()
					x.get_f()
			else:
				openlist=add_openlist(x)
				x.set_parent(nowpoint)
				x.get_g
				x.get_h
				x.get_f

		if((end in closelist)or(len(openlist)==0)):
			flag=false	
			
	
		
