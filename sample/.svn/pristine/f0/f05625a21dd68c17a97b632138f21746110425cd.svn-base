#  找出开启列表中f值最小的节点，作为当前节点
def get_nowpoint(end):
	minlen=100000
	nowpoint=None
	for x in range(openlist):
		x.get_g()
		x.get_h(end)
		x.get_f()
		if(x.f<minlen):
			minlen=x.f
			nowpoint=x				
	return nowpoint

